{
  "name": "scp-component-test-library",
  "version": "1.0.2",
  "description": "A component library for testing out DHIS2 Component Search functionality",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2designlab/scp-component-test-library.git"
  },
  "keywords": [
    "dhis2-component-search"
  ],
  "author": "h.heskja@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dhis2designlab/scp-component-test-library/issues"
  },
  "homepage": "https://github.com/dhis2designlab/scp-component-test-library#readme",
  "dependencies": {
    "@dhis2/app-runtime": "^2.4.0",
    "react": "^16.14.0"
  },
  "dhis2ComponentSearch": {
    "language": "react",
    "components": [
        {
            "name": "Organizational Unit Tree",
            "export": "OrgUnitTree",
            "description": "A simple OrgUnit Tree",
            "dhis2Version": [ "32.0.0", "32.1.0", "33.0.0" ]
        },
        {
            "name": "Button",
            "export": "Button",
            "description": "A generic button",
            "dhis2Version": [ "32.0.0", "32.1.0", "33.0.0" ]
        },
        {
            "name": "Covid-19 Visualizer",
            "export": "CovidVisualizer",
            "description": "Visualizer for Covid-19 data",
            "dhis2Version": [ "32.0.0", "32.1.0", "33.0.0" ]
        },
        {
            "name": "Headerbar",
            "export": "Headerbar",
            "description": "Just another header bar",
            "dhis2Version": [ "32.0.0", "32.1.0", "33.0.0" ]
        },
        {
            "name": "Tabs",
            "export": "Tabs",
            "description": "Tabs to structure data",
            "dhis2Version": [ "32.0.0", "32.1.0", "33.0.0" ]
        }
    ]
  }
}
